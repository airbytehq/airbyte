{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ID": {
      "type": "integer"
    },
    "Name": {
      "type": "string"
    },
    "ApiTokens": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Color": {
      "type": "string"
    },
    "SmtpApiActivated": {
      "type": "boolean"
    },
    "RawEmailEnabled": {
      "type": "boolean"
    },
    "DeliveryType": {
      "type": "string"
    },
    "ServerLink": {
      "type": "string"
    },
    "InboundAddress": {
      "type": "string"
    },
    "InboundHookUrl": {
      "type": "string"
    },
    "BounceHookUrl": {
      "type": "string"
    },
    "OpenHookUrl": {
      "type": "string"
    },
    "DeliveryHookUrl": {
      "type": "string"
    },
    "PostFirstOpenOnly": {
      "type": "boolean"
    },
    "InboundDomain": {
      "type": "string"
    },
    "InboundHash": {
      "type": "string"
    },
    "InboundSpamThreshold": {
      "type": "integer"
    },
    "TrackOpens": {
      "type": "boolean"
    },
    "TrackLinks": {
      "type": "string"
    },
    "IncludeBounceContentInHook": {
      "type": "boolean"
    },
    "ClickHookUrl": {
      "type": "string"
    },
    "EnableSmtpApiErrorHooks": {
      "type": "boolean"
    }
  }
}

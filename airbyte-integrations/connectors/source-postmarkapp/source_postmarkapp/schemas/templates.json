{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "TotalCount": {
      "description": "Total number of templates available.",
      "type": "integer"
    },
    "Templates": {
      "description": "Details of each template.",
      "type": "object",
      "properties": {
        "Active": {
          "description": "Flag indicating if the template is active.",
          "type": "boolean"
        },
        "TemplateId": {
          "description": "Unique identifier for the template.",
          "type": "integer"
        },
        "Name": {
          "description": "Name of the template.",
          "type": "string"
        },
        "Alias": {
          "description": "Alias or nickname for the template.",
          "type": "string"
        },
        "TemplateType": {
          "description": "Type or category of the template.",
          "type": "string"
        },
        "LayoutTemplate": {
          "description": "Content layout template used for the template.",
          "type": "string"
        }
      }
    }
  }
}

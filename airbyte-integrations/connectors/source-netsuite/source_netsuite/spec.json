{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/netsuite",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Netsuite Spec",
    "type": "object",
    "required": ["ACCOUNT", "CONSUMER_KEY", "CONSUMER_SECRET", "TOKEN_KEY", "TOKEN_SECRET"],
    "additionalProperties": false,
    "properties": {
      "ACCOUNT": {
        "type": "string",
        "description": "NetSuite account name"
      },
      "COMSUMER_KEY": {
        "type": "string",
        "description": "Consumer key value for TBA"
      },
      "CONSUMER_SECRET": {
        "type": "string",
        "description": "Consumer secret value for TBA"
      },
      "TOKEN_KEY": {
        "type": "string",
        "description": "Token key value for TBA"
      },
      "TOKEN_SECRET": {
        "type": "string",
        "description": "Token secret value for TBA"
      }
    }
  }
}

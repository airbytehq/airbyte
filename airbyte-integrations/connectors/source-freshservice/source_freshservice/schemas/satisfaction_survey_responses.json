{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "overall_rating": {
      "type": ["null", "integer"]
    },
    "overall_rating_text": {
      "type": ["null", "string"]
    },
    "primary_question": {
      "type": ["null", "string"]
    },
    "questionnaire_responses": {
      "type": ["null", "object"],
      "items": {
        "type": ["null", "object"],
        "additionalProperties": true,
        "properties": {
          "question": {
            "type": ["null", "object"],
            "additionalProperties": true,
            "properties": {
              "question_text": {
                "type": ["null", "string"]
              }
            }
          },
          "answers": {
            "type": ["null", "array"],
            "items": [
              {
                "type": ["null", "object"],
                "additionalProperties": true,
                "properties": {
                  "answer_text": {
                    "type": ["null", "string"]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "created_at": {
      "type": ["null", "string"]
    },
    "updated_at": {
      "type": ["null", "string"]
    }
  }
}

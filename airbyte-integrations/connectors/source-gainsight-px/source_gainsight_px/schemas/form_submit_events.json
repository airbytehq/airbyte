{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Form Submit Events Schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "eventId": {
            "type": [
                "null",
                "string"
            ]
        },
        "identifyId": {
            "type": [
                "null",
                "string"
            ]
        },
        "propertyKey": {
            "type": [
                "null",
                "string"
            ]
        },
        "date": {
            "type": [
                "null",
                "number"
            ],
            "format": "int64"
        },
        "eventType": {
            "type": [
                "null",
                "string"
            ]
        },
        "sessionId": {
            "type": [
                "null",
                "string"
            ]
        },
        "userType": {
            "type": [
                "null",
                "string"
            ],
            "enum": [
                "LEAD",
                "USER",
                "VISITOR",
                "EMPTY_USER_TYPE"
            ]
        },
        "accountId": {
            "type": [
                "null",
                "string"
            ]
        },
        "globalContext": {
            "type": "object",
            "properties": {
                "scheme": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "host": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "path": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "queryString": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "hash": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "queryParams": {
                    "type": "object",
                    "patternProperties": {
                        "^[0-9]+$": {
                            "type": [
                                "null",
                                "string"
                            ]
                        }
                    },
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        },
        "remoteHost": {
            "type": [
                "null",
                "string"
            ]
        },
        "referrer": {
            "type": [
                "null",
                "string"
            ]
        },
        "screenHeight": {
            "type": [
                "null",
                "number"
            ],
            "format": "int32"
        },
        "screenWidth": {
            "type": [
                "null",
                "number"
            ],
            "format": "int32"
        },
        "languages": {
            "type": "array",
            "items": {
                "type": [
                    "null",
                    "string"
                ]
            }
        },
        "pageTitle": {
            "type": [
                "null",
                "string"
            ]
        },
        "formData": {
            "type": "object",
            "additionalProperties": {
                "type": [
                    "null",
                    "string"
                ]
            }
        }
    }
}
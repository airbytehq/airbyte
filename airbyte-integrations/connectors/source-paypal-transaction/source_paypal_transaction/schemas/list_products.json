{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": ["null", "object"],
  "additionalProperties": true,
  "properties": {
    "id": { "type": ["null", "string"] },
    "name": { "type": ["null", "string"] },
    "description": { "type": ["null", "string"] },
    "create_time": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "href": { "type": ["null", "string"] },
          "rel": { "type": ["null", "string"] },
          "method": { "type": ["null", "string"] }
        }
      }
    }
  }
}

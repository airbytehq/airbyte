{
  "connectionSpecification": {
    "type": "object",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": ["base_url", "username"],
    "properties": {
      "base_url": {
        "type": "string",
        "order": 0,
        "title": "Base URL"
      },
      "username": {
        "type": "string",
        "title": "Username",
        "order": 1
      },
      "password": {
        "type": "string",
        "title": "Password",
        "always_show": true,
        "airbyte_secret": true,
        "order": 2
      }
    },
    "additionalProperties": true
  },
  "supportsNormalization": false,
  "supportsDBT": false
}

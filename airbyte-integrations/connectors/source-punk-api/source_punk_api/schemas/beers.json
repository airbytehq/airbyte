              "$id": "#root/items/ingredients/hops/items",
              "title": "Items",
              "type": "object",
              "required": ["name", "amount", "add", "attribute"],
              "properties": {
                "name": {
                  "$id": "#root/items/ingredients/hops/items/name",
                  "title": "Name",
                  "type": "string",
                  "default": "",
                  "examples": ["Fuggles"],
                  "pattern": "^.*$"
                },
                "amount": {
                  "$id": "#root/items/ingredients/hops/items/amount",
                  "title": "Amount",
                  "type": "object",
                  "required": ["value", "unit"],
                  "properties": {
                    "value": {
                      "$id": "#root/items/ingredients/hops/items/amount/value",
                      "title": "Value",
                      "type": ["null", "number", "integer"],
                      "examples": [25],
                      "default": 0
                    },
                    "unit": {
                      "$id": "#root/items/ingredients/hops/items/amount/unit",
                      "title": "Unit",
                      "type": "string",
                      "default": "",
                      "examples": ["grams"],
                      "pattern": "^.*$"
                    }
                  }
                },
                "add": {
                  "$id": "#root/items/ingredients/hops/items/add",
                  "title": "Add",
                  "type": "string",
                  "default": "",
                  "examples": ["start"],
                  "pattern": "^.*$"
                },
                "attribute": {
                  "$id": "#root/items/ingredients/hops/items/attribute",
                  "title": "Attribute",
                  "type": "string",
                  "default": "",
                  "examples": ["bitter"],
                  "pattern": "^.*$"
                }
              }
            }
          },
          "yeast": {
            "$id": "#root/items/ingredients/yeast",
            "title": "Yeast",
            "type": "string",
            "default": "",
            "examples": ["Wyeast 1056 - American Aleâ„¢"],
            "pattern": "^.*$"
          }
        }
      },
      "food_pairing": {
        "$id": "#root/items/food_pairing",
        "title": "Food_pairing",
        "type": "array",
        "default": [],
        "items": {
          "$id": "#root/items/food_pairing/items",
          "title": "Items",
          "type": "string",
          "default": "",
          "examples": ["Spicy chicken tikka masala"],
          "pattern": "^.*$"
        }
      },
      "brewers_tips": {
        "$id": "#root/items/brewers_tips",
        "title": "Brewers_tips",
        "type": "string",
        "default": "",
        "examples": [
          "The earthy and floral aromas from the hops can be overpowering. Drop a little Cascade in at the end of the boil to lift the profile with a bit of citrus."
        ],
        "pattern": "^.*$"
      },
      "contributed_by": {
        "$id": "#root/items/contributed_by",
        "title": "Contributed_by",
        "type": "string",
        "default": "",
        "examples": ["Sam Mason <samjbmason>"],
        "pattern": "^.*$"

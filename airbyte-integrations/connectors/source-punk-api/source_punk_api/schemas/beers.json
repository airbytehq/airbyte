{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/object1667075607.json",
  "title": "Root",
  "type": "array",
  "default": [],
  "items": {
    "$id": "#root/items",
    "title": "Items",
    "type": "object",
    "required": [
      "id",
      "name",
      "tagline",
      "first_brewed",
      "description",
      "image_url",
      "abv",
      "ibu",
      "target_fg",
      "target_og",
      "ebc",
      "srm",
      "ph",
      "attenuation_level",
      "volume",
      "boil_volume",
      "method",
      "ingredients",
      "food_pairing",
      "brewers_tips",
      "contributed_by"
    ],
    "properties": {
      "id": {
        "$id": "#root/items/id",
        "title": "Id",
        "type": "integer",
        "examples": [1],
        "default": 0
      },
      "name": {
        "$id": "#root/items/name",
        "title": "Name",
        "type": "string",
        "default": "",
        "examples": ["Buzz"],
        "pattern": "^.*$"
      },
      "tagline": {
        "$id": "#root/items/tagline",
        "title": "Tagline",
        "type": "string",
        "default": "",
        "examples": ["A Real Bitter Experience."],
        "pattern": "^.*$"
      },
      "first_brewed": {
        "$id": "#root/items/first_brewed",
        "title": "First_brewed",
        "type": "string",
        "default": "",
        "examples": ["09/2007"],
        "pattern": "^.*$"
      },
      "description": {
        "$id": "#root/items/description",
        "title": "Description",
        "type": "string",
        "default": "",
        "examples": [
          "A light, crisp and bitter IPA brewed with English and American hops. A small batch brewed only once."
        ],
        "pattern": "^.*$"
      },
      "image_url": {
        "$id": "#root/items/image_url",
        "title": "Image_url",
        "type": "string",
        "default": "",
        "examples": ["https://images.punkapi.com/v2/keg.png"],
        "pattern": "^.*$"
      },
      "abv": {
        "$id": "#root/items/abv",
        "title": "Abv",
        "type": "number",
        "examples": [4.5],
        "default": 0.0
      },
      "ibu": {
        "$id": "#root/items/ibu",
        "title": "Ibu",
        "type": ["null", "number", "integer"],
        "examples": [60],
        "default": 0
      },
      "target_fg": {
        "$id": "#root/items/target_fg",
        "title": "Target_fg",
        "type": "integer",
        "examples": [1010],
        "default": 0
      },
      "target_og": {
        "$id": "#root/items/target_og",
        "title": "Target_og",
        "type": ["null", "number", "integer"],
        "examples": [1044],
        "default": 0
      },
      "ebc": {
        "$id": "#root/items/ebc",
        "title": "Ebc",
        "type": ["null", "integer", "number"],
        "examples": [20],
        "default": 0
      },
      "srm": {
        "$id": "#root/items/srm",
        "title": "Srm",
        "type": ["number", "integer", "null"],
        "examples": [10],
        "default": 0
      },
      "ph": {
        "$id": "#root/items/ph",
        "title": "Ph",
        "type": ["null", "number", "integer"],
        "examples": [4.4],
        "default": 0.0
      },
      "attenuation_level": {
        "$id": "#root/items/attenuation_level",
        "title": "Attenuation_level",
        "type": ["number", "integer"],
        "examples": [75],
        "default": 0
      },
      "volume": {
        "$id": "#root/items/volume",
        "title": "Volume",
        "type": "object",
        "required": ["value", "unit"],
        "properties": {
          "value": {
            "$id": "#root/items/volume/value",
            "title": "Value",
            "type": "integer",
            "examples": [20],
            "default": 0
          },
          "unit": {
            "$id": "#root/items/volume/unit",
            "title": "Unit",
            "type": "string",
            "default": "",
            "examples": ["litres"],
            "pattern": "^.*$"
          }
        }
      },
      "boil_volume": {
        "$id": "#root/items/boil_volume",
        "title": "Boil_volume",
        "type": "object",
        "required": ["value", "unit"],
        "properties": {
          "value": {
            "$id": "#root/items/boil_volume/value",
            "title": "Value",
            "type": "integer",
            "examples": [25],
            "default": 0
          },
          "unit": {
            "$id": "#root/items/boil_volume/unit",
            "title": "Unit",
            "type": "string",
            "default": "",
            "examples": ["litres"],
            "pattern": "^.*$"
          }
        }
      },
      "method": {
        "$id": "#root/items/method",
        "title": "Method",
        "type": "object",
        "required": ["mash_temp", "fermentation", "twist"],
        "properties": {
          "mash_temp": {
            "$id": "#root/items/method/mash_temp",
            "title": "Mash_temp",
            "type": "array",
            "default": [],
            "items": {
              "$id": "#root/items/method/mash_temp/items",
              "title": "Items",
              "type": "object",
              "required": ["temp", "duration"],
              "properties": {
                "temp": {
                  "$id": "#root/items/method/mash_temp/items/temp",
                  "title": "Temp",
                  "type": "object",
                  "required": ["value", "unit"],
                  "properties": {
                    "value": {
                      "$id": "#root/items/method/mash_temp/items/temp/value",
                      "title": "Value",
                      "type": "integer",
                      "examples": [64],
                      "default": 0
                    },
                    "unit": {
                      "$id": "#root/items/method/mash_temp/items/temp/unit",
                      "title": "Unit",
                      "type": "string",
                      "default": "",
                      "examples": ["celsius"],
                      "pattern": "^.*$"
                    }
                  }
                },
                "duration": {
                  "$id": "#root/items/method/mash_temp/items/duration",
                  "title": "Duration",
                  "type": ["number", "integer", "null"],
                  "examples": [75],
                  "default": 0
                }
              }
            }
          },
          "fermentation": {
            "$id": "#root/items/method/fermentation",
            "title": "Fermentation",
            "type": "object",
            "required": ["temp"],
            "properties": {
              "temp": {
                "$id": "#root/items/method/fermentation/temp",
                "title": "Temp",
                "type": "object",
                "required": ["value", "unit"],
                "properties": {
                  "value": {
                    "$id": "#root/items/method/fermentation/temp/value",
                    "title": "Value",
                    "type": "integer",
                    "examples": [19],
                    "default": 0
                  },
                  "unit": {
                    "$id": "#root/items/method/fermentation/temp/unit",
                    "title": "Unit",
                    "type": "string",
                    "default": "",
                    "examples": ["celsius"],
                    "pattern": "^.*$"
                  }
                }
              }
            }
          },
          "twist": {
            "$id": "#root/items/method/twist",
            "title": "Twist",
            "type": ["string", "null"],
            "default": null
          }
        }
      },
      "ingredients": {
        "$id": "#root/items/ingredients",
        "title": "Ingredients",
        "type": "object",
        "required": ["malt", "hops", "yeast"],
        "properties": {
          "malt": {
            "$id": "#root/items/ingredients/malt",
            "title": "Malt",
            "type": "array",
            "default": [],
            "items": {
              "$id": "#root/items/ingredients/malt/items",
              "title": "Items",
              "type": "object",
              "required": ["name", "amount"],
              "properties": {
                "name": {
                  "$id": "#root/items/ingredients/malt/items/name",

                  "type": "string",
                  "default": "",
                  "examples": ["Maris Otter Extra Pale"],
                  "pattern": "^.*$"
                },
                "amount": {
                  "$id": "#root/items/ingredients/malt/items/amount",
                  "title": "Amount",
                  "type": "object",
                  "required": ["value", "unit"],
                  "properties": {
                    "value": {
                      "$id": "#root/items/ingredients/malt/items/amount/value",
                      "title": "Value",
                      "type": "number",
                      "examples": [3.3],
                      "default": 0.0
                    },
                    "unit": {
                      "$id": "#root/items/ingredients/malt/items/amount/unit",
                      "title": "Unit",
                      "type": "string",
                      "default": "",
                      "examples": ["kilograms"],
                      "pattern": "^.*$"
                    }
                  }
                }
              }
            }
          },
          "hops": {
            "$id": "#root/items/ingredients/hops",
            "title": "Hops",
            "type": "array",
            "default": [],

{
  "documentationUrl": "https://docs.biggeo.io/integrations/destinations/biggeo",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination BigGeo",
    "type": "object",
    "required": ["api_key"],
    "additionalProperties": true,
    "properties": {
      "api_key": {
        "title": "API Key",
        "type": "string",
        "description": "API key to use for authentication to BigGeo.",
        "airbyte_secret": true
      },
      "batch_size": {
        "title": "Batch Size",
        "type": "integer",
        "description": "Number of records to batch before sending to the API. Default is 1000.",
        "default": 1000,
        "minimum": 1,
        "maximum": 10000
      }
    }
  },
  "supportsIncremental": true,
  "supportsNormalization": false,
  "supportsDBT": false,
  "supported_destination_sync_modes": ["overwrite", "append"]
}

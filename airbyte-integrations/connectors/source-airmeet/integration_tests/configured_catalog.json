{
  "streams": [
    {
      "stream": {
        "name": "airmeets",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "uid": {
              "type": ["string", "null"],
              "description": "Unique identifier for the Airmeet"
            },
            "name": {
              "type": ["string", "null"],
              "description": "Name of the Airmeet"
            },
            "description": {
              "type": ["string", "null"],
              "description": "Description of the Airmeet"
            },
            "status": {
              "type": ["string", "null"],
              "description": "Current status of the Airmeet"
            },
            "start_time": {
              "type": ["string", "null"],
              "format": "date-time"
            },
            "end_time": {
              "type": ["string", "null"],
              "format": "date-time"
            },
            "created_at": {
              "type": ["string", "null"],
              "format": "date-time"
            },
            "updated_at": {
              "type": ["string", "null"],
              "format": "date-time"
            },
            "timezone": {
              "type": ["string", "null"]
            },
            "registration_link": {
              "type": ["string", "null"]
            },
            "join_link": {
              "type": ["string", "null"]
            },
            "host_email": {
              "type": ["string", "null"]
            }
          }
        },
        "supported_sync_modes": ["full_refresh"],
        "source_defined_primary_key": [["uid"]]
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite"
    }
  ]
}
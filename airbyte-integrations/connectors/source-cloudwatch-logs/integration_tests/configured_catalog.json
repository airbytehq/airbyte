{
  "streams": [
    {
      "stream": {
        "name": "app_logs",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "timestamp": {
              "type": "integer"
            },
            "message": {
              "type": "string"
            },
            "logStreamName": {
              "type": "string"
            },
            "ingestionTime": {
              "type": "integer"
            },
            "eventId": {
              "type": "string"
            }
          }
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "source_defined_cursor": true,
        "default_cursor_field": ["timestamp"],
        "is_resumable": true
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append"
    }
  ]
}

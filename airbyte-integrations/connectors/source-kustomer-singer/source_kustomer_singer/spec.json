{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/kustomer",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Source Kustomer Singer Spec",
    "type": "object",
    "required": ["api_token", "start_date", "user_agent"],
    "additionalProperties": true,
    "properties": {
      "api_token": {
        "title": "API Token",
        "type": "string",
        "description": "Kustomer API Token. See the <a href=\"https://developer.kustomer.com/kustomer-api-docs/reference/authentication\">docs</a> on how to obtain this",
        "airbyte_secret": true
      },
      "start_date": {
        "title": "Start Date",
        "type": "string",
        "description": "The date from which you'd like to replicate the data",
        "examples": ["2019-01-01T00:00:00Z"]
      },
      "user_agent": {
        "type": "string",
        "title": "User Agent",
        "description": "Kustomer API User Agent",
        "examples": ["tap-kustomer <api_user_email@your_company.com>"]
      }
    }
  }
}

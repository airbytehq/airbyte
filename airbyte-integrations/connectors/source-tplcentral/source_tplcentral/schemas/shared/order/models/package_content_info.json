{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "PackageContentId": {
      "type": "integer"
    },
    "PackageId": {
      "type": "integer"
    },
    "OrderItemId": {
      "type": "integer"
    },
    "ReceiveItemId": {
      "type": "integer"
    },
    "OrderItemPickExceptionId": {
      "type": ["null", "integer"]
    },
    "Qty": {
      "type": "number"
    },
    "LotNumber": {
      "type": "string"
    },
    "SerialNumber": {
      "type": "string"
    },
    "ExpirationDate": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "CreateDate": {
      "type": "string",
      "format": "date-time"
    },
    "SerialNumbers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}

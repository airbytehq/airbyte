{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "in GETs: the contact; in POSTs or PUTs: identifies existing contact or properties of new contact ",
  "type": "object",
  "properties": {
    "contact_id": {
      "type": [
        "null",
        "integer"
      ]
    },
    "company_name": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "fax": {
      "type": "string"
    },
    "email_address": {
      "type": "string"
    },
    "dept": {
      "type": "string"
    },
    "code": {
      "description": "for identifying: if known specify only this; if other fields also specified it is not used to identify",
      "type": "string"
    }
  }
}

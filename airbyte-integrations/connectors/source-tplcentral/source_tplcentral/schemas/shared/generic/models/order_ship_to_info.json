{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "retailer_info": {
      "description": "in GETs: the retailer; in POSTs or PUTs: identifies existing retailer or properties of new retailer",
      "$ref": "retailer_info.json"
    },
    "same_as": {
      "description": "PUT or POST: to use same Contact as another contact on the same order; GET: always null",
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "../../common/enum/contact_type.json"
        }
      ]
    },
    "retailer_id": {
      "description": "DEPRECATED, will be removed from model: for ShipTo, use RetailerInfo; for SoldTo and BillTo, ignored",
      "type": [
        "null",
        "integer"
      ]
    },
    "is_quick_lookup": {
      "type": "boolean"
    },
    "contact_id": {
      "description": "for identifying: if known specify only this; other fields are ignored",
      "type": [
        "null",
        "integer"
      ]
    },
    "company_name": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "fax": {
      "type": "string"
    },
    "email_address": {
      "type": "string"
    },
    "dept": {
      "type": "string"
    },
    "code": {
      "description": "for identifying: if known specify only this; if other fields also specified it is not used to identify",
      "type": "string"
    },
    "address_status": {
      "description": "confirmation status",
      "$ref": "../../common/enum/address_status_type.json"
    }
  }
}

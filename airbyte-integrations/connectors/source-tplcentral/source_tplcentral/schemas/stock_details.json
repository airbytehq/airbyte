{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cursor": {
      "type": "string",
      "format": "date-time"
    },
    "receive_item_id": {
      "type": "integer"
    },
    "item_identifier": {
      "$ref": "generic_models_item_identifier.json"
    },
    "description": {
      "type": "string"
    },
    "description2": {
      "type": "string"
    },
    "upc": {
      "type": "string"
    },
    "qualifier": {
      "type": "string"
    },
    "received": {
      "type": "number"
    },
    "available": {
      "description": "unallocated; see IsOnHold for whether it's allocatable",
      "type": "number"
    },
    "is_on_hold": {
      "description": "true if receive item explicitly held",
      "type": "boolean"
    },
    "quarantined": {
      "description": "true if implicitly held because of located in quarantine",
      "type": "boolean"
    },
    "on_hand": {
      "type": "number"
    },
    "secondary_received": {
      "type": [
        "null",
        "number"
      ]
    },
    "secondary_available": {
      "description": "unallocated; see IsOnHold for whether it's allocatable",
      "type": [
        "null",
        "number"
      ]
    },
    "lot_number": {
      "type": "string"
    },
    "serial_number": {
      "type": "string"
    },
    "expiration_date": {
      "type": [
        "null",
        "string"
      ],
      "format": "date-time"
    },
    "cost": {
      "type": [
        "null",
        "number"
      ]
    },
    "supplier_identifier": {
      "$ref": "generic_models_contact_identifier.json"
    },
    "location_identifier": {
      "$ref": "generic_models_location_identifier.json"
    },
    "pallet_identifier": {
      "$ref": "generic_models_pallet_identifier.json"
    },
    "inventory_unit_of_measure_identifier": {
      "$ref": "generic_models_unit_of_measure_identifier.json"
    },
    "secondary_unit_of_measure_identifier": {
      "$ref": "generic_models_unit_of_measure_identifier.json"
    },
    "inventory_units_per_secondary_unit": {
      "type": [
        "null",
        "number"
      ]
    },
    "receiver_id": {
      "type": "integer"
    },
    "received_date": {
      "type": "string",
      "format": "date-ime"
    },
    "reference_num": {
      "type": "string"
    },
    "po_num": {
      "type": "string"
    },
    "trailer_number": {
      "type": "string"
    },
    "saved_elements": {
      "type": "array",
      "items": {
        "$ref": "generic_models_saved_element.json"
      }
    },
    "weight_imperial": {
      "type": [
        "null",
        "number"
      ]
    },
    "weight_metric": {
      "type": [
        "null",
        "number"
      ]
    }
  }
}

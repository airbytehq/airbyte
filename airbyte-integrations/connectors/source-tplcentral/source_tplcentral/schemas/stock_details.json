{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "integer"
    },
    "_cursor": {
      "type": "string",
      "format": "date-time"
    },
    "receive_item_id": {
      "type": "integer"
    },
    "item_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sku": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "description": {
      "type": "string"
    },
    "description2": {
      "type": "string"
    },
    "upc": {
      "type": "string"
    },
    "qualifier": {
      "type": "string"
    },
    "received": {
      "type": "number"
    },
    "available": {
      "description": "unallocated; see IsOnHold for whether it's allocatable",
      "type": "number"
    },
    "is_on_hold": {
      "description": "true if receive item explicitly held",
      "type": "boolean"
    },
    "quarantined": {
      "description": "true if implicitly held because of located in quarantine",
      "type": "boolean"
    },
    "on_hand": {
      "type": "number"
    },
    "secondary_received": {
      "type": ["null", "number"]
    },
    "secondary_available": {
      "description": "unallocated; see IsOnHold for whether it's allocatable",
      "type": ["null", "number"]
    },
    "lot_number": {
      "type": "string"
    },
    "serial_number": {
      "type": "string"
    },
    "expiration_date": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "cost": {
      "type": ["null", "number"]
    },
    "supplier_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "location_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name_key": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "facility_identifier": {
              "$schema": "http://json-schema.org/draft-07/schema#",
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "name": {
                  "type": "string"
                },
                "id": {
                  "type": "integer"
                }
              }
            },
            "name": {
              "type": "string"
            }
          }
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "pallet_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name_key": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "facility_identifier": {
              "$schema": "http://json-schema.org/draft-07/schema#",
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "name": {
                  "type": "string"
                },
                "id": {
                  "type": "integer"
                }
              }
            },
            "name": {
              "type": "string"
            }
          }
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "inventory_unit_of_measure_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "secondary_unit_of_measure_identifier": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "inventory_units_per_secondary_unit": {
      "type": ["null", "number"]
    },
    "receiver_id": {
      "type": "integer"
    },
    "received_date": {
      "type": "string",
      "format": "date-ime"
    },
    "reference_num": {
      "type": "string"
    },
    "po_num": {
      "type": "string"
    },
    "trailer_number": {
      "type": "string"
    },
    "saved_elements": {
      "type": "array",
      "items": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "weight_imperial": {
      "type": ["null", "number"]
    },
    "weight_metric": {
      "type": ["null", "number"]
    }
  }
}

{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Jenkins Spec",
    "type": "object",
    "required": [
      "server_url",
      "user",
      "token"
    ],
    "additionalProperties": false,
    "properties": {
      "server_url": {
        "type": "string",
        "title": "Jenkins Server URL",
        "examples": [
          "https://my-jenkins-server.example.com"
        ]
      },
      "user": {
        "type": "string",
        "title": "Jenkins User"
      },
      "token": {
        "type": "string",
        "title": "Jenkins Token",
        "airbyte_secret": true
      }
    }
  }
}
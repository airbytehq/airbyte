{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "self": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "completeDate": {
      "type": "string",
      "format": "date-time"
    },
    "originBoardId": {
      "type": "integer"
    },
    "boardId": {
      "type": "integer",
      "description": "Used to determine which board the sprint is a part of. (Not always the same as originBoardId)"
    },
    "goal": {
      "type": "string"
    },
    "createdDate": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}

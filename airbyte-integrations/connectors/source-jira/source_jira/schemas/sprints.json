      "type": "integer"
    },
    "self": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "completeDate": {
      "type": "string",

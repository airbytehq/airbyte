        "number",
        "null"
      ],
      "format": "double"
    },
    "CostCenter": {
      "type": [
        "null",
        "string"
      ]
    },
    "CostCenterSettings": {
      "type": "string",
      "enum": [
        "ALLOWED",
        "MANDATORY",
        "NOTALLOWED"
      ]
    },
    "Description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 200
    },
    "Number": {
      "type": "integer",
      "format": "int32",
      "minimum": 1000,
      "maximum": 9999
    },
    "Project": {
      "type": "string"
    },
    "ProjectSettings": {
      "type": "string",
      "enum": [
        "ALLOWED",
        "MANDATORY",
        "NOTALLOWED"
      ]

      "format": "int32"
    },
    "Year": {
      "type": "integer",
      "format": "int32"
    },
    "VATCode": {
      "type": [
        "string",
        "null"
      ]
    },
    "BalanceCarriedForward": {
      "type": "number",
      "format": "double"
    },
    "TransactionInformation": {
      "type": "string"
    },
    "TransactionInformationSettings": {
      "type": "string",
      "enum": [
        "ALLOWED",
        "MANDATORY",
        "NOTALLOWED"
      ]
    },
    "QuantitySettings": {
      "type": "string",
      "enum": [
        "ALLOWED",
        "MANDATORY",
        "NOTALLOWED"
      ]
    },
    "QuantityUnit": {
      "type": "string"
    },
    "OpeningQuantities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Project": {
            "type": "string"
          },
          "Balance": {
            "type": "integer",
            "format": "int32"
          }
        }
      }
    }

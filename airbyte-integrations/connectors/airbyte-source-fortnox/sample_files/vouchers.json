{
  "streams": [
    {
      "stream": {
        "name": "vouchers",
        "json_schema": {
          "type": "object",
          "properties": {
            "@url": {
              "type": "string"
            },
            "Comments": {
              "type": "string"
            },
            "Description": {
              "type": "string"
            },
            "ReferenceNumber": {
              "type": "string"
            },
            "ReferenceType": {
              "type": "string",
              "enum": [
                "INVOICE",
                "SUPPLIERINVOICE",
                "INVOICEPAYMENT",
                "SUPPLIERPAYMENT",
                "MANUAL",
                "CASHINVOICE",
                "ACCRUAL"
              ]
            },
            "TransactionDate": {
              "type": "string",
              "format": "date"
            },
            "VoucherNumber": {
              "type": "integer",
              "format": "int32"
            },
            "VoucherSeries": {
              "type": "string"
            },
            "Year": {
              "type": "integer",
              "format": "int32"
            },
            "ApprovalState": {
              "type": "integer",
              "format": "int32"
            }
          },
          "$schema": "http://json-schema.org/schema#"
        },
        "supported_sync_modes": [
          "full_refresh",
          "incremental"
        ]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append"
    }
  ]
}
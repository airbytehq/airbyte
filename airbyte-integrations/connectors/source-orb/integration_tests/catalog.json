                "line2": { "type": ["null", "string"] },
                "postal_code": { "type": ["null", "string"] },
                "state": { "type": ["null", "string"] }
              }
            },
            "billing_address": {
              "type": ["null", "object"],
              "properties": {
                "city": { "type": ["null", "string"] },

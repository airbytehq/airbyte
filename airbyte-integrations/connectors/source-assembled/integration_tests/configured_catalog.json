{
  "streams": [
    {
      "stream": {
        "name": "people",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "agent_id": { "type": ["string", "null"] },
            "first_name": { "type": ["string", "null"] },
            "last_name": { "type": ["string", "null"] },
            "email": { "type": ["string", "null"] },
            "imported_id": { "type": ["string", "null"] },
            "role": { "type": ["string", "null"] },
            "agent_role": { "type": ["string", "null"] },
            "channels": { "type": "array", "items": [{ "type": "string" }] },
            "site": { "type": ["string", "null"] },
            "teams": { "type": "array", "items": [{ "type": "string" }] },
            "queues": { "type": "array", "items": [{ "type": "string" }] },
            "skills": { "type": "array", "items": [{ "type": "string" }] },
            "platforms": {
              "type": "object",
              "properties": {
                "zendesk": { "type": ["string", "null"] },
                "api": { "type": ["string", "null"] }
              }
            },
            "start_date": { "type": ["integer", "null"] },
            "end_date": { "type": ["integer", "null"] },
            "created_at": { "type": ["integer", "null"] },
            "deleted": { "type": ["boolean", "null"] },
            "staffable": { "type": ["boolean", "null"] }
          }
        },
        "supported_sync_modes": ["full_refresh"],
        "source_defined_primary_key": [["id"]]
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite"
    },
    {
      "stream": {
        "name": "activities",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "start_time": { "type": ["integer", "null"] },
            "end_time": { "type": ["integer", "null"] },
            "agent_id": { "type": ["string", "null"] },
            "type_id": { "type": ["string", "null"] }
          }
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "source_defined_cursor": true,
        "default_cursor_field": ["start_time"],
        "source_defined_primary_key": [["id"]]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append"
    }
  ]
}

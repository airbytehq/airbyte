{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Microsoft Teams Spec",
    "type": "object",
    "required": ["tenant_id", "client_id", "client_secret", "start_date"],
    "additionalProperties": false,
    "properties": {
      "tenant_id": {
        "type": "string",
        "description": "Application (client) ID",
        "airbyte_secret": true
      },
      "client_id": {
        "type": "string",
        "description": "Directory (tenant) ID",
        "airbyte_secret": true
      },
      "client_secret": {
        "type": "string",
        "description": "Client secret",
        "airbyte_secret": true
      },
      "start_date": {
        "type": "string",
        "description": "The date from which you'd like to get details about Microsoft Teams device usage by users, in the format YYYY-MM-DDT00:00:00Z.",
        "examples": ["2020-12-01T00:00:00Z"],
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
      }
    }
  }
}

      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": ["null", "string", "integer"]
        },
        "name": {
          "type": ["null", "string"]
        },
        "symbol": {
          "type": ["null", "string"]
        }
      }
    },
    "currencyRate": {
      "type": ["null", "number"]
    },
    "isSplitTransaction": {
      "type": ["null", "boolean"]
    },
    "parentTransactionId": {
      "type": ["null", "string"]
    },
    "subTotal": {
      "type": ["null", "number"]
    },
    "taxAmount": {
      "type": ["null", "number"]
    },
    "totalAmount": {
      "type": ["number", "string"]
    }
  }

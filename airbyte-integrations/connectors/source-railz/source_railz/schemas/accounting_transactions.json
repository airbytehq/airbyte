{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["string", "integer"]
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "transactionNumber": {
      "type": ["null", "string"]
    },
    "postedDate": {
      "type": "string",
      "format": "date-time"
    },
    "entityRef": {
      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": ["null", "string", "integer"]
        },
        "name": {
          "type": ["null", "string"]
        }
      }
    },
    "type": {
      "type": "string"
    },
    "isPosting": {
      "type": "boolean"
    },
    "isReconciled": {
      "type": ["null", "boolean"]
    },
    "accountRef": {
      "type": ["null", "object"],
      "properties": {

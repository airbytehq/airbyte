{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["string", "integer"]
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "code": {
      "type": ["null", "string"]
    },
    "effectiveTaxRate": {
      "type": ["null", "number"]
    },
    "totalTaxRate": {
      "type": ["null", "number"]
    },
    "components": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "name": {
            "type": ["null", "string"]
          },
          "rate": {
            "type": ["null", "number"]
          },
          "isCompound": {
            "type": ["null", "boolean"]
          },
          "isPurchasesTax": {
            "type": ["null", "boolean"]
          },
          "isSalesTax": {
            "type": ["null", "boolean"]
          }
        }
      }
    },
    "sourceModifiedDate": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}

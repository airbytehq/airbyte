{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["query", "date", "site_url", "device", "country"],
  "properties": {
    "site_url": {
      "type": ["string"]
    },
    "search_type": {
      "type": ["null", "string"]
    },
    "date": {
      "type": ["null", "string"],
      "format": "date"
    },
    "country": {
      "type": ["string"]
    },
    "device": {
      "type": ["string"]
    },
    "page": {
      "type": ["string"]
    },
    "query": {
      "type": ["string"]
    },
    "clicks": {
      "type": ["null", "integer"]
    },
    "impressions": {
      "type": ["null", "integer"]
    },
    "ctr": {
      "type": ["null", "number"],
      "multipleOf": 1e-25
    },
    "position": {
      "type": ["null", "number"],
      "multipleOf": 1e-25
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["query", "date", "site_url"],
  "properties": {
    "site_url": {
      "type": ["string"]
    },
    "search_type": {
      "type": ["null", "string"]
    },
    "date": {
      "type": ["string"],
      "format": "date"
    },
    "query": {
      "type": ["string"]
    },
    "clicks": {
      "type": ["null", "integer"]
    },
    "impressions": {
      "type": ["null", "integer"]
    },
    "ctr": {
      "type": ["null", "number"],
    },
    "position": {
      "type": ["null", "number"],
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "anyOf": [
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "ENQUEUED"
        },
        "data": {
          "type": "object"
        },
        "event": {
          "type": "string"
        },
        "profile": {
          "type": "object"
        },
        "override": {
          "type": "object"
        },
        "recipient": {
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "MAPPED"
        },
        "event_id": {
          "type": "string"
        },
        "notification_id": {
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "PROFILE_LOADED"
        },
        "merged_profile": {
          "type": "object"
        },
        "received_profile": {
          "type": "object"
        },
        "stored_profile": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "RENDERED"
        },
        "output": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "type": "string",
          "enum": [
            "CLICKED",
            "DELIVERED",
            "ENQUEUED",
            "OPENED",
            "SENT",
            "UNDELIVERABLE",
            "UNMAPPED",
            "UNROUTABLE"
          ]
        },
        "channel": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "label": {
              "type": "string"
            }
          }
        },
        "integration": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "DELIVERED"
        },
        "channel": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "label": {
              "type": "string"
            }
          }
        },
        "integration": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "UNDELIVERABLE"
        },
        "channel": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "label": {
              "type": "string"
            }
          }
        },
        "integration": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "UNDELIVERABLE"
        },
        "reason": {
          "type": "string",
          "enum": [
            "FILTERED",
            "NO_CHANNELS",
            "NO_PROVIDERS",
            "PROVIDER_ERROR",
            "UNPUBLISHED",
            "UNDELIVERABLE",
            "UNSUBSCRIBED"
          ]
        },
        "reasonCode": {
          "type": "string",
          "enum": ["HARD", "SOFT"]
        }
      }
    },
    {
      "type": "object",
      "properties": {
        "ts": {
          "type": "number"
        },
        "type": {
          "const": "UNROUTABLE"
        },
        "reason": {
          "type": "string",
          "enum": [
            "FILTERED",
            "NO_CHANNELS",
            "NO_PROVIDERS",
            "PROVIDER_ERROR",
            "UNPUBLISHED",
            "UNDELIVERABLE",
            "UNSUBSCRIBED"
          ]
        }
      }

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": ["null", "string"]
    },
    "email": {
      "type": ["null", "string"]
    },
    "firstName": {
      "type": ["null", "string"]
    },
    "lastName": {
      "type": ["null", "string"]
    },
    "companyName": {
      "type": ["null", "string"]
    },
    "companyId": {
      "type": ["null", "string"]
    },
    "tags": {
      "type": "array",
      "items": {
        "type": ["null", "string"]
      }
    },
    "createdDate": {
      "type": ["null", "string"],
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
    },
    "properties": {
      "type": "array",
      "items": {
        "type": ["null", "object"],
        "additionalProperties": true,
        "properties": {
          "label": {
            "type": ["null", "string"]
          },
          "name": {
            "type": ["null", "string"]
          },
          "type": {
            "type": ["null", "string"]
          },
          "value": {
            "type": ["null", "string"]
          }
        }
      }
    }
  }
}

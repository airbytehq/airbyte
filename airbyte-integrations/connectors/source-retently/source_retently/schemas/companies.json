{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": ["null", "string"]
    },
    "createdDate": {
      "type": ["null", "string"],
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
    },
    "domain": {
      "type": ["null", "string"]
    },
    "companyName": {
      "type": ["null", "string"]
    },
    "industryName": {
      "type": ["null", "string"]
    },
    "tags": {
      "type": "array",
      "items": {
        "type": ["null", "string"]
      }
    },
    "cxMetrics": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "NPS": {
          "type": ["null", "number"]
        },
        "CSAT": {
          "type": ["null", "number"]
        },
        "CES": {
          "type": ["null", "number"]
        },
        "STAR": {
          "type": ["null", "number"]
        }
      }
    },
    "contactsCount": {
      "type": ["null", "number"]
    }
  }
}

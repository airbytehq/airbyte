{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/mobilize",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Mobilize Spec",
    "type": "object",
    "required": ["org_id"],
    "additionalProperties": false,
    "properties": {
      "org_id": {
        "type": "string",
        "description": "The Mobilize id (numeric) of the organization you want to load.",
        "pattern": "^[0-9]+$",
        "examples": ["1, 100, 999"]
      }
    }
  }
}

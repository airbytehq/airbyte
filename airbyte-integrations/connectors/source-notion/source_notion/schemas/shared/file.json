{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": ["null", "object"],
  "additionalProperties": true,
  "properties": {
    "type": {
      "enum": ["file", "external"]
    },
    "caption": {
      "type": ["null", "array"],
      "items": {
        "$ref": "rich_text.json"
      }
    },
    "external": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "url": {
          "type": "string"
        }
      }
    },
    "file": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "url": {
          "type": "string"
        },
        "expiry_time": {
          "type": "string"
        }
      }
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": ["null", "object"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "color": {
      "enum": [
        "default",
        "gray",
        "brown",
        "orange",
        "yellow",
        "green",
        "blue",
        "purple",
        "pink",
        "red"
      ]
    }
  }
}

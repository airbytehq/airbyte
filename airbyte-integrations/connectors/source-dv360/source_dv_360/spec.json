{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Display & Video 360 Spec",
    "type": "object",
    "required": [
      "credentials",
      "partner_id",
      "start_date"
    ],
    "additionalProperties": false,
    "properties": {
      "credentials": {
        "type": "object",
        "description": "Oauth2 credentials",
        "required": [
          "access_token",
          "refresh_token",
          "token_uri",
          "client_id",
          "client_secret"
        ],
        "properties": {
          "access_token": {
            "type": "string",
            "description": "",
            "airbyte_secret": true
          },
          "refresh_token": {
            "type": "string",
            "description": "",
            "airbyte_secret": true
          },
          "token_uri": {
            "type": "string",
            "description": "",
            "airbyte_secret": true
          },
          "client_id": {
            "type": "string",
            "description": "",
            "airbyte_secret": true
          },
          "client_secret": {
            "type": "string",
            "description": "",
            "airbyte_secret": true
          }
        }
      },
      "partner_id": {
        "type": "integer",
        "description": ""
      },
      "start_date": {
        "type": "string",
        "description": "",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
      },
      "end_date": {
        "type": "string",
        "description": "",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
      }
    }
  }
}
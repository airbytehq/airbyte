          "#NA",
          "-1.#IND",
          "-1.#QNAN",
          "-NaN",
          "-nan",
          "1.#IND",
          "1.#QNAN",
          "N/A",
          "NA",
          "NULL",
          "NaN",
          "n/a",
          "nan",
          "null"
        ],
        "true_values": ["1", "True", "TRUE", "true"],
        "false_values": ["0", "False", "FALSE", "false"],
        "inference_type": "Primitive Types Only",
        "strings_can_be_null": false,

        "FRIDAY",
        "MONDAY",
        "SATURDAY",
        "SUNDAY",
        "THURSDAY",
        "TUESDAY",
        "UNKNOWN",
        "UNSPECIFIED",
        "WEDNESDAY"
      ]
    },
    "segments.month": {
      "type": ["string"]
    },
    "segments.week": {
      "type": ["string"]
    },
    "segments.quarter": {
      "type": ["string"]
    },
    "segments.year": {
      "type": ["integer"]
    },
    "segments.ad_network_type": {
      "type": ["string"],
      "enum": [
        "CONTENT",
        "MIXED",
        "SEARCH",
        "SEARCH_PARTNERS",
        "UNKNOWN",
        "UNSPECIFIED",
        "YOUTUBE_SEARCH",
        "YOUTUBE_WATCH"

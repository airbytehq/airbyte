    "campaign.id": {
      "type": ["integer"]
    },
    "campaign.name": {
      "type": ["string"]
    },
    "campaign.status": {
      "type": ["string"],
      "enum": [
        "ENABLED",
        "PAUSED",
        "REMOVED",
        "UNKNOWN",
        "UNSPECIFIED"
      ]
    },
    "ad_group.name": {
      "type": ["string"]
    },
    "ad_group.status": {
      "type": ["string"],
      "enum": [
        "ENABLED",
        "PAUSED",

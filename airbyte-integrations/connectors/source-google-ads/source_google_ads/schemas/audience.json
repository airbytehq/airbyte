{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "audience.description": {
      "type": ["null", "string"]
    },
    "audience.dimensions": {
      "type": ["null", "array"],
      "audience_segments": {
        "type": "string"
      }
    },
    "audience.exclusion_dimension": {
      "type": ["null", "array"],
      "exclusions": {
        "type": "string"
      }
    },
    "audience.id": {
      "type": ["null", "integer"]
    },
    "audience.name": {
      "type": ["null", "string"]
    },
    "audience.resource_name": {
      "type": ["null", "string"]
    },
    "audience.status": {
      "type": ["null", "string"],
      "enum": [
        "ENABLED",
        "REMOVED",
        "UNKNOWN",
        "UNSPECIFIED"
      ]
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["segments.date", "ad_group.id", "geographic_view.country_criterion_id"],
  "properties": {
    "customer.descriptive_name": {
      "type": ["null", "string"]
    },
    "geographic_view.country_criterion_id": {
      "type": ["integer"]
    },
    "geographic_view.location_type": {
      "type": ["null", "string"],
      "enum": [
        "AREA_OF_INTEREST",
        "LOCATION_OF_PRESENCE",
        "UNKNOWN",
        "UNSPECIFIED"
      ]
    },
    "ad_group.id": {
      "type": ["integer"]
    },
    "segments.date": {
      "type": ["string"],
      "format": "date"
    }
  }
}

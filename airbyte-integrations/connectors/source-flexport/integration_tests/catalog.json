              "state": {
                "type": [
                  "null",
                  "string"
                ],
                "description": "State/County/Province/Region."
              },
              "country": {
                "type": [
                  "null",
                  "string"
                ],
                "description": "Country."
              },
              "country_code": {
                "type": [
                  "null",
                  "string"
                ],
                "description": "Two-letter country code (ISO 3166-1 Alpha-2)."
              },
              "zip": {
                "type": [
                  "null",

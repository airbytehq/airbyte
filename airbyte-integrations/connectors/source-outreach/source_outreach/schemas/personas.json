{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "createdAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "updatedAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "prospects": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "integer"]
      }
    }
  }
}
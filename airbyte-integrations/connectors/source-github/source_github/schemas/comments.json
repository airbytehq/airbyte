{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id"],
  "properties": {
    "repository": {
      "type": ["string"]
    },
    "id": {
      "type": ["integer"]
    },
    "node_id": {
      "type": ["null", "string"]
    },
    "user": {
      "$ref": "user.json"
    },
    "url": {
      "type": ["null", "string"]
    },
    "html_url": {
      "type": ["null", "string"]
    },
    "body": {
      "type": ["null", "string"]
    },
    "user_id": {
      "type": ["null", "integer"]
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "updated_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "issue_url": {
      "type": ["null", "string"]
    },
    "author_association": {
      "type": ["null", "string"]
    }
  }
}

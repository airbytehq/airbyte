{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repository": {
      "type": "string"
    },
    "name": {
      "type": ["null", "string"]
    },
    "commit": {
      "type": ["null", "object"],
      "properties": {
        "sha": {
          "type": ["null", "string"]
        },
        "url": {
          "type": ["null", "string"]
        }
      }
    },
    "protected": {
      "type": ["null", "boolean"]
    },
    "protection": {
      "type": ["null", "object"],
      "properties": {
        "required_status_checks": {
          "type": ["null", "object"],
          "properties": {
            "enforcement_level": {
              "type": ["null", "string"]
            },
            "contexts": {
              "type": ["null", "array"],
              "items": {
                "type": ["null", "string"]
              }
            }
          }
        }
      }
    },
    "protection_url": {
      "type": ["null", "string"]
    }
  }
}

{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repository": {
      "type": "string"
    },
    "name": {
      "type": ["null", "string"]
    },
    "commit": {
      "type": ["null", "object"],
      "properties": {
        "sha": {
          "type": ["null", "string"]
        },
        "url": {
          "type": ["null", "string"]
        }
      }
    },
    "zipball_url": {
      "type": ["null", "string"]
    },
    "tarball_url": {
      "type": ["null", "string"]
    },
    "node_id": {
      "type": ["null", "string"]
    }
  }
}

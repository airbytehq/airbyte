{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["user_id"],
  "properties": {
    "repository": {
      "type": ["string"]
    },
    "user_id": {
      "type": ["integer"]
    },
    "starred_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "user": {
      "$ref": "user.json"
    }
  }
}

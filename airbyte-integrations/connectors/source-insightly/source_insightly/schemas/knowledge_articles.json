    "Title": {
      "type": ["string", "null"]
    },
    "Body": {
      "type": ["string", "null"]
    },
    "URL_SLUG": {
      "type": ["string", "null"]
    },
    "DOWNVOTE_COUNT": {
      "type": ["integer", "null"]

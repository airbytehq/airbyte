{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "PRODUCT_ID": {
      "type": "integer"
    },
    "PRODUCT_NAME": {
      "type": ["string", "null"]
    },
    "PRODUCT_CODE": {
      "type": ["string", "null"]
    },
    "PRODUCT_SKU": {
      "type": ["string", "null"]
    },
    "DESCRIPTION": {
      "type": ["string", "null"]
    },
    "PRODUCT_FAMILY": {
      "type": ["string", "null"]
    },
    "PRODUCT_IMAGE_URL": {
      "type": ["string", "null"]
    },
    "CURRENCY_CODE": {
      "type": ["string", "null"]
    },
    "DEFAULT_PRICE": {
      "type": ["integer", "null"]
    },
    "DATE_CREATED_UTC": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "DATE_UPDATED_UTC": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "CREATED_USER_ID": {
      "type": ["integer", "null"]
    },
    "OWNER_USER_ID": {
      "type": ["integer", "null"]
    },
    "ACTIVE": {
      "type": ["boolean", "null"]
    },
    "CUSTOMFIELDS": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "FIELD_NAME": {
            "type": ["string", "null"]
          },
          "FIELD_VALUE": {
            "type": "object"
          }
        }
      }
    }
  }
}

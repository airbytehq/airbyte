{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "TICKET_ID": {
      "type": "integer"
    },
    "ORGANISATION_ID": {
      "type": "integer"
    },
    "TICKET_TYPE": {
      "type": "string"
    },
    "SUBJECT": {
      "type": "string"
    },
    "TICKET_STATUS": {
      "type": "string"
    },
    "PRIORITY": {
      "type": "string"
    },
    "TO_EMAIL_ADDRESS": {
      "type": "string"
    },
    "CONTACT_ID": {
      "type": "integer"
    },
    "CREATED_USER_ID": {
      "type": "integer"
    },
    "OWNER_USER_ID": {
      "type": "integer"
    },
    "DATE_CREATED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "DATE_UPDATED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "TICKET_NUMBER": {
      "type": "string"
    },
    "SOURCE": {
      "type": "string"
    },
    "DATE_SOLVED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "DATE_CLOSED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "TicketCommentBodyHtml": {
      "type": "string"
    },
    "CUSTOMFIELDS": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "FIELD_NAME": {
              "type": "string"
            },
            "FIELD_VALUE": {
              "type": "object"
            }
          }
        }
      ]
    },
    "TAGS": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "TAG_NAME": {
              "type": "string"
            }
          }
        }
      ]
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "EMAIL_ID": {
      "type": "integer"
    },
    "EMAIL_FROM": {
      "type": "string"
    },
    "SUBJECT": {
      "type": "string"
    },
    "EMAIL_DATE_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "FORMAT": {
      "type": "string"
    },
    "SIZE": {
      "type": "integer"
    },
    "OWNER_USER_ID": {
      "type": "integer"
    },
    "DATE_CREATED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "QUEUED_SEND_DATE_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "CREATED_USER_ID": {
      "type": "integer"
    },
    "TAGS": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "TAG_NAME": {
              "type": "string"
            }
          }
        }
      ]
    },
    "LINKS": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "LINK_ID": {
              "type": "integer"
            },
            "OBJECT_NAME": {
              "type": "string"
            },
            "OBJECT_ID": {
              "type": "integer"
            },
            "LINK_OBJECT_NAME": {
              "type": "string"
            },
            "LINK_OBJECT_ID": {
              "type": "integer"
            },
            "ROLE": {
              "type": "string"
            },
            "DETAILS": {
              "type": "string"
            },
            "RELATIONSHIP_ID": {
              "type": "integer"
            },
            "IS_FORWARD": {
              "type": "boolean"
            }
          }
        }
      ]
    }
  }
}

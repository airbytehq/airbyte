{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "NOTE_ID": {
      "type": "integer"
    },
    "TITLE": {
      "type": "string"
    },
    "BODY": {
      "type": "string"
    },
    "DATE_CREATED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "DATE_UPDATED_UTC": {
      "type": "string",
      "format": "date-time"
    },
    "OWNER_USER_ID": {
      "type": "integer"
    },
    "LINKS": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "LINK_ID": {
              "type": "integer"
            },
            "OBJECT_NAME": {
              "type": "string"
            },
            "OBJECT_ID": {
              "type": "integer"
            },
            "LINK_OBJECT_NAME": {
              "type": "string"
            },
            "LINK_OBJECT_ID": {
              "type": "integer"
            },
            "ROLE": {
              "type": "string"
            },
            "DETAILS": {
              "type": "string"
            },
            "RELATIONSHIP_ID": {
              "type": "integer"
            },
            "IS_FORWARD": {
              "type": "boolean"
            }
          }
        }
      ]
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "name": "Event",
  "type": "object",
  "properties": {
    "id": {
      "type": ["string", "null"],
      "maxLength": 40
    },
    "occurred_at": {
      "type": ["integer", "null"]
    },
    "source": {
      "type": ["string", "null"]
    },
    "user": {
      "type": ["string", "null"],
      "maxLength": 150
    },
    "event_type": {
      "type": ["string", "null"]
    },
    "api_version": {
      "type": ["string", "null"]
    },
    "webhook_status": {
      "type": ["string", "null"]
    },
    "content": {
      "type": ["object", "null"]
    },
    "webhooks": {
      "type": ["array", "null"],
      "items": {
        "type": ["object", "null"],
        "properties": {
          "id": {
            "type": ["string", "null"],
            "maxLength": 40
          },
          "webhook_status": {
            "type": ["string", "null"]
          }
        }
      }
    },
    "custom_fields": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "name": {
            "type": ["null", "string"]
          },
          "value": {
            "type": ["null", "string"]
          }
        }
      }
    }
  }
}

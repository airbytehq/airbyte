{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": ["null", "string"]
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "creative_type": {
      "type": ["null", "string"]
    },
    "is_standard": {
      "type": ["null", "boolean"]
    },
    "is_owner": {
      "type": ["null", "boolean"]
    },
    "product_tags": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "string"]
      }
    },
    "dominant_color": {
      "type": ["null", "string"]
    },
    "parent_pin_id": {
      "type": ["null", "string"]
    },
    "link": {
      "type": ["null", "string"]
    },
    "title": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "alt_text": {
      "type": ["null", "string"]
    },
    "note": {
      "type": ["null", "string"]
    },
    "board_id": {
      "type": ["null", "string"]
    },
    "board_section_id": {
      "type": ["null", "string"]
    },
    "board_owner": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "username": {
          "type": ["null", "string"]
        }
      }
    },
    "media": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "media_type": {
          "type": ["null", "string"]
        }
      }
    },
    "pin_metrics": {
      "type": ["null", "object"]
    },
    "has_been_promoted": {
      "type": ["null", "boolean"]
    }
  }
}

{
  "documentationUrl": "https://docs.airbyte.io/integrations/destinations/ngpvan",
  "supported_destination_sync_modes": ["overwrite"],
  "supportsIncremental": false,
  "supportsDBT": false,
  "supportsNormalization": false,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Ngpvan",
    "type": "object",
    "required": ["van_api_key","bulk_import_type","gcs_bucket","local_test"],
    "additionalProperties": true,
    "properties": {
      "bulk_import_type": {
        "title": "Bulk import type",
        "type": "string",
        "description": "Choose the type of bulk import you are running.",
        "examples": [
          "Contacts","Activist Codes"
        ],
        "order": 1
      },
      "van_api_key": {
        "title": "VAN API Key",
        "type": "string",
        "description": "VAN API Key.",
        "airbyte_secret": true,
        "order": 2
      },
      "service_account_key": {
        "title": "Service account key",
        "type": "string",
        "description": "Keyfile JSON for authenticating as a service account with GCS access & blob writing permissions.",
        "airbyte_secret": true,
        "order": 3
      },
      "gcs_bucket": {
        "title": "GCS bucket",
        "type": "string",
        "description": "GCS bucket where data will be staged/stored.",
        "order": 4
      },
      "local_test": {
        "title": "local_test",
        "type": "boolean",
        "order": 5
      }
    }
  }
}
    "type": { "type": "string" },
    "id": { "type": "string" },
    "attributes": {
      "type": ["null", "object"],
      "properties": {
        "name": { "type": "string" },
        "created": { "type": "string", "format": "date-time" },
        "updated": { "type": "string", "format": "date-time" },

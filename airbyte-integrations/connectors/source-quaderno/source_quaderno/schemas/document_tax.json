{
  "type": "object",
  "description": "The taxes associated with the invoice.",
  "properties": {
    "label": {
      "type": "string",
      "nullable": true,
      "description": "Name of the tax applied"
    },
    "rate": {
      "type": "number",
      "format": "float",
      "default": 0,
      "description": "The rate of tax applied."
    },
    "country": {
      "type": "string",
      "description": "Tax country",
      "example": "CA"
    },
    "region": {
      "type": "string",
      "nullable": true,
      "description": "Tax state/province. Only for US sales tax and Canada GST.",
      "example": "BC"
    },
    "county": {
      "type": "string",
      "nullable": true,
      "description": "Tax county. Only for US sales tax."
    },
    "transaction_type": {
      "type": "string",
      "enum": [
        "consulting",
        "eservice",
        "ebook",
        "saas",
        "standard",
        "reduced"
      ],
      "nullable": true,
      "description": "Type of transaction",
      "example": "eservice"
    },
    "amount_cents": {
      "type": "integer",
      "description": "Tax amount in cents.",
      "example": 1000
    }
  },
  "$schema": "http://json-schema.org/schema#"
}
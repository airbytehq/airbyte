{
  "streams": [
    {
      "stream": {
        "name": "question_stream",
        "source_defined_cursor": true,
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "data": {
              "type": "array",
              "properties": {
                "allow_other": {
                  "type": "boolean"
                },
                "customer_type": {
                  "type": "string"
                },
                "frequency_type": {
                  "type": "string"
                },
                "id": {
                  "type": "integer"
                },
                "inserted_at": {
                  "type": "string"
                },
                "other_placeholder": {
                  "type": "string"
                },
                "prompt": {
                  "type": "string"
                },
                "published_at": {
                  "type": ["string", "null"]
                },
                "randomize_responses": {
                  "type": "boolean"
                },
                "responses": {
                  "type": "array",
                  "properties": {
                    "clarification_question": {
                      "type": [
                        "null",
                        "string"
                      ]
                    },
                    "id": {
                      "type": "integer"
                    },
                    "value": {
                      "type": "string"
                    }
                  }
                },
                "submit_text": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "updated_at": {
                  "type": "string"
                }
              }
            }
          }
        },
        "supported_sync_modes": ["full_refresh", "incremental"],
        "default_cursor_field": ["inserted_at"],
        "source_defined_primary_key": [["id"]]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append"
    }
  ]
}

{
  "streams": [
    {
      "name": "question_stream",
      "json_schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "properties": {
              "allow_other": {
                "type": "boolean"
              },
              "customer_type": {
                "type": "string"
              },
              "frequency_type": {
                "type": "string"
              },
              "id": {
                "type": "integer"
              },
              "inserted_at": {
                "type": "string"
              },
              "other_placeholder": {
                "type": "string"
              },
              "prompt": {
                "type": "string"
              },
              "published_at": {
                "type": ["null", "string"]
              },
              "randomize_responses": {
                "type": "boolean"
              },
              "responses": {
                "type": "array",
                "properties": {
                  "clarification_question": {
                    "type": [
                      "null",
                      "string"
                    ]
                  },
                  "id": {
                    "type": "integer"
                  },
                  "value": {
                    "type": "string"
                  }
                }
              },
              "submit_text": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "updated_at": {
                "type": "string"
              }
            }
          }
        }
      },
      "supported_sync_modes": [
        "full_refresh"
      ]
    }
  ]
}

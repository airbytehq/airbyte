{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "name": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "auto_created": {
      "type": ["null", "boolean"]
    },
    "visibility": {
      "type": ["null", "string"]
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}

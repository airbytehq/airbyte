{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "name": {
      "type": ["null", "string"]
    },
    "email": {
      "type": ["null", "string"]
    },
    "phone": {
      "type": ["null", "string"]
    },
    "admin": {
      "type": ["null", "boolean"]
    },
    "owner": {
      "type": ["null", "boolean"]
    },
    "agency_owner": {
      "type": ["null", "boolean"]
    },
    "verified": {
      "type": ["null", "boolean"]
    },
    "role_id": {
      "type": ["null", "integer"]
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}

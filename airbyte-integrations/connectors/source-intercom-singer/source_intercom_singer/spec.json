{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/intercom",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Source Intercom Singer Spec",
    "type": "object",
    "required": ["user_agent", "api_key"],
    "additionalProperties": false,
    "properties": {
      "user_agent": {
        "type": "string",
        "description": "Intercom user agent"
      },
      "api_key": {
        "type": "string",
        "description": "Intercom API Key. See the <a href=\"https://docs.airbyte.io/integrations/sources/intercom\">docs</a> for more information on how to obtain this key."
      }
    }
  }
}

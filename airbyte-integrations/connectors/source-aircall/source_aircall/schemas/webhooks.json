{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Webhooks Schema",
  "additionalProperties": true,
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "number"]
    },
    "direct_link": {
      "type": ["null", "string"]
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "url": {
      "type": ["null", "string"]
    },
    "active": {
      "type": ["null", "boolean"]
    },
    "token": {
      "type": ["null", "string"]
    },
    "events": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "string"]
      }
    }
  }
}

    "id": {
      "type": ["null", "number"]
    },
    "first_name": {
      "type": ["null", "string"]
    },
    "last_name": {
      "type": ["null", "string"]
    },
    "company_name": {
      "type": ["null", "string"]
    },
    "information": {
      "type": ["null", "string"]
    },
    "is_shared": {
      "type": ["null", "boolean"]
    },
    "direct_link": {
      "type": ["null", "string"]
    },
    "created_at": {
      "type": ["null", "number"]
    },
    "updated_at": {
      "type": ["null", "number"]
    },
    "phone_numbers": {
      "type": ["null", "array"],

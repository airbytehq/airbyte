{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "eventID": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          },
          "key": {
            "type": "string"
          }
        }
      }
    },
    "dateCreated": {
      "type": "string",
      "format": "date-time"
    },
    "user": {
      "type": ["null", "object"],
      "properties": {
        "username": {
          "type": ["null", "string"]
        },
        "name": {
          "type": ["null", "string"]
        },
        "ip_address": {
          "type": ["null", "string"]
        },
        "email": {
          "type": ["null", "string"]
        },
        "data": {
          "type": ["null", "object"],
          "properties": {
            "isStaff": {
              "type": "boolean"
            }
          }
        },
        "id": {
          "type": "string"
        }
      }
    },
    "message": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "platform": {
      "type": "string"
    },
    "event.type": {
      "type": "string"
    },
    "groupID": {
      "type": "string"
    },
    "title": {
      "type": "string"
    }
  }
}

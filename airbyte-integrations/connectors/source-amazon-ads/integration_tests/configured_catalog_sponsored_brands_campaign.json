                    {
                      "title": "SBStoreLandingPage",
                      "pageType": "string",
                      "url": "string"
                    },
                    {
                      "title": "SBCustomLandingPage",
                      "pageType": "string",
                      "url": "string"
                    }
                  ]
                }
              },
              {
                "title": "SBVideoCampaignResponseCommon",
                "campaignId": "integer",
                "name": "string",
                "tag": "string",
                "budget": "number",
                "budgetType": {
                  "type": "string",
                  "enum": ["lifetime", "daily"]
                },
                "startDate": "string",
                "endDate": "string",
                "state": {
                  "type": "string",
                  "enum": ["enabled", "paused", "archived"]

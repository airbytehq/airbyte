{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "field_type": {
      "type": "string"
    },
    "priority": {
      "type": "integer"
    },
    "value_type": {
      "type": "string"
    },
    "private": {
      "type": "boolean"
    },
    "required": {
      "type": "boolean"
    },
    "require_approval": {
      "type": "boolean"
    },
    "trigger_new_version": {
      "type": "boolean"
    },
    "name_key": {
      "type": "string"
    },
    "description": {
      "type": ["null", "string"]
    },
    "expose_in_job_board_api": {
      "type": "boolean"
    },
    "api_only": {
      "type": "boolean"
    },
    "offices": {
      "type": "array"
    },
    "departments": {
      "type": "array"
    },
    "template_token_string": {
      "type": ["null", "string"]
    },
    "custom_field_options": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "priority": {
            "type": "integer"
          },
          "external_id": {
            "type": ["null", "string"]
          }
        }
      }
    }
  }
}

{
  "documentationUrl": "https://docs.airbyte.com/integrations/sources/marketo",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Source Marketo Spec",
    "type": "object",
    "required": ["domain_url", "client_id", "client_secret", "start_date"],
    "additionalProperties": true,
    "properties": {
      "domain_url": {
        "title": "Domain URL",
        "type": "string",
        "order": 3,
        "examples": ["https://000-AAA-000.mktorest.com"]
      },
      "client_id": {
        "title": "Client ID",
        "type": "string",
        "order": 0,
        "airbyte_secret": true
      },
      "client_secret": {
        "title": "Client Secret",
        "type": "string",
        "order": 1,
        "airbyte_secret": true
      },
      "start_date": {
        "title": "Start Date",
        "type": "string",
        "order": 2,
        "examples": ["2020-09-25T00:00:00Z"],
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
      }
    }
  }
}

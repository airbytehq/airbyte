{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "company": {
      "title": "company",
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "founder": {
          "type": "string"
        },
        "founded": {
          "type": "number"
        },
        "employees": {
          "type": "number"
        },
        "vehicles": {
          "type": "number"
        },
        "launch_sites": {
          "type": "number"
        },
        "test_sites": {
          "type": "number"
        },
        "ceo": {
          "type": "string"
        },
        "cto": {
          "type": "string"
        },
        "coo": {
          "type": "string"
        },
        "cto_propulsion": {
          "type": "string"
        },
        "valuation": {
          "type": "number"
        },
        "headquarters": {
          "title": "headquarters",
          "type": "object",
          "properties": {
            "address": {
              "type": "string"
            },
            "city": {
              "type": "string"
            },
            "state": {
              "type": "string"
            }
          }
        },
        "links": {
          "title": "links",
          "type": "object",
          "properties": {
            "website": {
              "type": "string"
            },
            "flickr": {
              "type": "string"
            },
            "twitter": {
              "type": "string"
            },
            "elon_twitter": {
              "type": "string"
            }
          }
        },
        "summary": {
          "type": "string"
        }
      }
    },
    "id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    }
  }
}

{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Outbrain Amplify Api Spec",
    "type": "object",
    "required": ["USERNAME", "PASSWORD", "MARKETER_ID","START_DATE"],
    "additionalProperties": false,
    "properties": {
      "USERNAME": {
        "type": "string",
        "description": "Outbrain Username"
      },
      "PASSWORD": {
        "type": "string",
        "description": "Outbrain Password"
      },
      "MARKETER_ID": {
        "type": "string",
        "description": "Outbrain Marketer ID"
      },
      "START_DATE": {
        "type": "string",
        "description": "Start Data for data retrieval"
      },
      "END_DATE": {
        "type": "string",
        "description": "End Date for data retrieval"
      }
    }
  }
}

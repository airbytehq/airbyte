    "title": "Outbrain Amplify Api Spec",
    "type": "object",
    "required": ["USERNAME", "PASSWORD", "MARKETER_ID"],
    "additionalProperties": false,
    "properties": {
      "USERNAME": {
        "type": "string",
        "description": "Outbrain Username"
      },
      "PASSWORD": {
        "type": "string",
        "description": "Outbrain Password"
      },
      "MARKETER_ID": {
        "type": "string",
        "description": "Outbrain Marketer ID"
      }
    }
  }
}

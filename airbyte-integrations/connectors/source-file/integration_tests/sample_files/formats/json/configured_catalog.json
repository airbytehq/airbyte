{
  "streams": [
    {
      "stream": {
        "name": "test",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "$schema": "http://json-schema.org/schema#",
            "type": "object",
            "properties": {
              "first_property": { "type": "string" },
              "countries": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "isoCode": {
                      "type": "string"
                    }
                  },
                  "required": ["isoCode", "name"]
                }
              }
            },
            "required": ["countries"]
          }
        }
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite"
    }
  ]
}

                "type" : "object",
                "additionalProperties" : true,
                "properties" : {
                  "codec" : {
                    "type" : "string",
                    "enum" : [ "snappy" ],
                    "default" : "snappy"
                  }
                },
                "title" : "snappy",
                "required" : [ "codec" ]
              } ],

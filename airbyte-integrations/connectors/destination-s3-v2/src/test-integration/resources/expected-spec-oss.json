                    "default" : "bzip2"
                  }
                },
                "title" : "bzip2",
                "required" : [ "codec" ]
              }, {
                "type" : "object",
                "additionalProperties" : true,
                "properties" : {
                  "codec" : {
                    "type" : "string",
                    "enum" : [ "xz" ],
                    "default" : "xz"
                  },
                  "compression_level" : {
                    "type" : "integer",
                    "title" : "Compression Level"
                  }
                },
                "title" : "xz",
                "required" : [ "codec", "compression_level" ]
              }, {
                "type" : "object",
                "additionalProperties" : true,
                "properties" : {
                  "codec" : {
                    "type" : "string",

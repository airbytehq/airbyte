{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/okta",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Okta Spec",
    "type": "object",
    "required": ["token", "base_url"],
    "additionalProperties": false,
    "properties": {
      "token": {
        "type": "string",
        "title": "API Token",
        "description": "A Okta token. See the <a href=\"https://docs.airbyte.io/integrations/sources/okta\">docs</a> for instructions on how to generate it.",
        "airbyte_secret": true
      },
      "base_url": {
        "type": "string",
        "title": "Base URL",
        "description": "The Okta base URL.",
        "airbyte_secret": true
      }
    }
  }
}

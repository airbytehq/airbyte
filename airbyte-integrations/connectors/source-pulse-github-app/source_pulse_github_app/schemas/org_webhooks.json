{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "active": {
      "type": "boolean",
      "description": "Whether the webhook is active"
    },
    "config": {
      "type": "object",
      "description": "Configuration settings for the webhook",
      "properties": {
        "content_type": {
          "type": "string",
          "description": "Content type of the webhook payload"
        },
        "insecure_ssl": {
          "type": "string",
          "description": "SSL verification settings"
        },
        "secret": {
          "type": "string",
          "description": "Secret token for webhook authentication"
        },
        "url": {
          "type": "string",
          "description": "URL where webhook notifications are sent",
          "format": "uri"
        }
      },
      "required": ["content_type", "insecure_ssl", "secret", "url"]
    },
    "created_at": {
      "type": "string",
      "description": "ISO 8601 timestamp of when the webhook was created",
      "format": "date-time"
    },
    "deliveries_url": {
      "type": "string",
      "description": "URL for webhook delivery information",
      "format": "uri"
    },
    "events": {
      "type": "array",
      "description": "List of events the webhook is subscribed to",
      "items": {
        "type": "string"
      }
    },
    "id": {
      "type": "integer",
      "description": "Unique identifier for the webhook"
    },
    "name": {
      "type": "string",
      "description": "Name of the webhook"
    },
    "org": {
      "type": "string",
      "description": "Organization name the webhook belongs to"
    },
    "ping_url": {
      "type": "string",
      "description": "URL for pinging the webhook",
      "format": "uri"
    },
    "type": {
      "type": "string",
      "description": "Type of the webhook",
    },
    "updated_at": {
      "type": "string",
      "description": "ISO 8601 timestamp of when the webhook was last updated",
      "format": "date-time"
    },
    "url": {
      "type": "string",
      "description": "API URL for the webhook",
      "format": "uri"
    }
  },
  "additionalProperties": true
}
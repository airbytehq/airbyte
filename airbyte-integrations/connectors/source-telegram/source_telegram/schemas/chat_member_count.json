{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "chat_id": {
      "type": ["null", "string"]
    },
    "chat_name": {
      "type": ["null", "string"]
    },
    "chat_member_count": {
      "type": ["null", "integer"]
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "airbyte_type": "timestamp_with_timezone"
    }
  }
}

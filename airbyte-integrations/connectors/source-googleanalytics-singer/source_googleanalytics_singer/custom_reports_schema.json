{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "dimensions", "metrics"],
    "properties": {
      "name": {
        "type": "string"
      },
      "dimensions": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "maxItems": 7,
        "uniqueItems": true
      },
      "metrics": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "maxItems": 10,
        "uniqueItems": true
      }
    }
  }
}

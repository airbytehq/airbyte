{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Twitter Ads Spec",
    "type": "object",
    "required": ["api-key","api-key-secret","access-token","access-token-secret", "account_id"],
    "additionalProperties": false,
    "properties": {
      "api-key": {
        "type": "string",
        "description": "API Key"
      },
      "api-key-secret": {
        "type": "string",
        "description": "API Key Secret."
      },
      "access-token": {
        "type": "string",
        "description": "API Access Token.."
      },
      "access-token-secret": {
        "type": "string",
        "description": "API Access Token secret."
      },
      "account_id": {
        "type": "string",
        "description": "Twitter account id."
      }
    }
  }
}

{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Twitter Ads Spec",
    "type": "object",
    "required": [ "CONSUMER_KEY","CONSUMER_SECRET","ACCESS_TOKEN","ACCESS_TOKEN_SECRET", "ACCOUNT_ID", "START_TIME"],
    "additionalProperties": true,
    "properties": {
      "CONSUMER_KEY": {
        "type": "string",
        "description": "API Key"
      },
      "CONSUMER_SECRET": {
        "type": "string",
        "description": "API Key Secret."
      },
      "ACCESS_TOKEN": {
        "type": "string",
        "description": "API Access Token.."
      },
      "ACCESS_TOKEN_SECRET": {
        "type": "string",
        "description": "API Access Token secret."
      },
      "ACCOUNT_ID": {
        "type": "string",
        "description": "Twitter account id."
      },
      "START_TIME": {
        "type": "string",
        "description": "Start time of data to be pulled."
      },
      "END_TIME": {
        "type": "string",
        "description": "End time of data to be pulled."
      },
      "GRANULARITY": {
        "type": "string",
        "description": "Granularity of data to be pulled."
      },
      "METRIC_GROUPS": {
        "type": "array",
        "description": "Metric Groups to be pulled."
      },
      "PLACEMENT": {
        "type": "string",
        "description": "Placement of data to be pulled."
      },
      "Segmentation": {
        "type": "string",
        "description": "Segmentation of data to be pulled."
      }
    }
  }
}

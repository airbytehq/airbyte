{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "is_admin": {
      "type": "boolean"
    },
    "is_reviewer": {
      "type": "boolean"
    },
    "languages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lang_name": {
            "type": "string"
          },
          "lang_id": {
            "type": "integer"
          },
          "is_writable": {
            "type": "boolean"
          },
          "lang_iso": {
            "type": "string"
          }
        }
      }
    },
    "user_id": {
      "type": "integer"
    },
    "created_at_timestamp": {
      "type": "integer"
    },
    "created_at": {
      "type": "string"
    },
    "admin_rights": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "fullname": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  }
}

{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/mailgun",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Source Mailgun Spec",
    "type": "object",
    "required": ["private_key"],
    "additionalProperties": true,
    "properties": {
      "private_key": {
        "type": "string",
        "description": "Private API key for authenticate with every request",
        "airbyte_secret": true
      },
      "start_timestamp": {
        "type": "number",
        "description": "Initial timestamp to gather data from"
      }
    }
  }
}

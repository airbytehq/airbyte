{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "StockLocationId": {
      "type": "string",
      "description": "Location ID"
    },
    "StockLocationIntId": {
      "type": "integer",
      "description": "Stock location integer id"
    },
    "LocationName": {
      "type": "string",
      "description": "Location name"
    },
    "IsFulfillmentCenter": {
      "type": "boolean",
      "description": "If location is a fulfillment center"
    },
    "LocationTag": {
      "type": "string",
      "description": "Location tag"
    },
    "BinRack": {
      "type": ["null", "string"],
      "description": "Bin rack"
    },
    "IsWarehouseManaged": {
      "type": ["null", "boolean"],
      "description": "If the location is warehouse managed."
    },
    "Address1": {
      "type": ["null", "string"]
    },
    "Address2": {
      "type": ["null", "string"]
    },
    "City": {
      "type": ["null", "string"]
    },
    "County": {
      "type": ["null", "string"]
    },
    "Country": {
      "type": ["null", "string"]
    },
    "ZipCode": {
      "type": ["null", "string"]
    },
    "CountInOrderUntilAcknowledgement": {
      "type": ["null", "boolean"]
    },
    "FulfilmentCenterDeductStockWhenProcessed": {
      "type": ["null", "boolean"]
    },
    "IsNotTrackable": {
      "type": ["null", "boolean"]
    }
  }
}

{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Gas Prices Spec",
    "type": "object",
    "required": ["api_key", "state"],
    "additionalProperties": false,
    "properties": {
      "api_key": {
        "type": "string",
        "description": "Your API key for authentication to this API"
      },
      "state": {
        "type": "string",
        "description": "The code of the State you want to get gas prices for",
        "examples": ["CA", "AK", "AL" , "WA"]
      }
    }
  }
}

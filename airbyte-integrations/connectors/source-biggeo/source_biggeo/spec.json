{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/biggeo",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Source BigGeo",
    "type": "object",
    "required": ["api_key"],
    "additionalProperties": false,
    "properties": {
      "api_key": {
        "type": "string",
        "description": "API Key for authenticating with BigGeo",
        "airbyte_secret": true,
        "order": 0
      },
      "data_source_name": {
        "type": "string",
        "description": "Name of the data source to retrieve from BigGeo. If not specified, all available data sources will be synced.",
        "order": 1
      }
    }
  }
}

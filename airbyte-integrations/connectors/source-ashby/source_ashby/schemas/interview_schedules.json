      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": ["null", "string"]
          },
          "interviewId": {
            "type": ["null", "string"]
          },
          "interviewScheduleId": {
            "type": ["null", "string"]
          },
          "interviewerUserIds": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "createdAt": {
            "type": ["null", "string"],
            "format": "date-time"
          },
          "startTime": {
            "type": ["null", "string"],
            "format": "date-time"
          },
          "endTime": {
            "type": ["null", "string"],
            "format": "date-time"
          },
          "feedbackLink": {
            "type": ["null", "string"]
          },
          "location": {
            "type": ["null", "string"]
          },
          "meetingLink": {
            "type": ["null", "string"]
          }
        }
      }
    }
  }

{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/bamboo-hr",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Bamboo HR Spec",
    "type": "object",
    "required": ["subdomain", "api_key"],
    "additionalProperties": false,
    "properties": {
      "subdomain": {
        "type": "string",
        "title": "Subdomain",
        "description": "Your company subdomain name used to access BambooHR."
      },
      "api_key": {
        "type": "string",
        "title": "API Key",
        "description": "Bamboo HR API Key. See the <a href=\"https://docs.airbyte.io/integrations/sources/bamboo-hr\">docs</a> for more information on how to obtain this key.",
        "airbyte_secret": true
      }
    }
  }
}

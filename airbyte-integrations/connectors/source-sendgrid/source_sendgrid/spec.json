{
  "documentationUrl": "https://docs.airbyte.com/integrations/sources/sendgrid",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Sendgrid Spec",
    "type": "object",
    "required": ["apikey"],
    "additionalProperties": true,
    "properties": {
      "apikey": {
        "title": "Sendgrid API key",
        "airbyte_secret": true,
        "type": "string",
        "order": 0
      },
      "start_time": {
        "title": "Start time",
        "type": "integer",
        "examples": [
          1558359837
        ],
        "order": 1
      }
    }
  }
}

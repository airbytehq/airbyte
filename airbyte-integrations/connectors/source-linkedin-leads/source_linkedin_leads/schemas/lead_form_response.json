{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id"],
  "properties": {
    "id": {
      "type": "string"
    },
    "owner": {
      "type": ["null", "object"],
      "properties": {
        "sponsoredAccount": {
          "type": ["null", "string"]
        }
      }
    },
    "leadType": {
      "type": ["null", "string"]
    },
    "testLead": {
      "type": ["null", "boolean"]
    },
    "submitter": {
      "type": ["null", "string"]
    },
    "submittedAt": {
      "type": ["null", "integer"]
    },
    "formResponse": {
      "type": ["null", "object"],
      "properties": {
        "answers": {
          "type": ["null", "array"],

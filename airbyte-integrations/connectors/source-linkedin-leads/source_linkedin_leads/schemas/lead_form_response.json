          "type": ["null", "array"]
        }
      }
    },
    "leadMetadata": {
      "type": ["null", "object"],
      "properties": {
        "sponsoredLeadMetadata": {
          "type": ["null", "object"],
          "properties": {
            "campaign": {
              "type": ["null", "string"]
            }
          }
        }
      }

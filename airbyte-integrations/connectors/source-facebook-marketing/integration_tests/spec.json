        "pattern_descriptor": "The Ad Account ID must be a number.",
        "examples": ["111111111111111"],
        "minItems": 1,
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "uniqueItems": true
      },
      "access_token": {

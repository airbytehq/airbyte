  "properties": {
    "_id": {
      "type": "string"
    },
    "name": {
      "type": ["null", "string"]
    },
    "userIds": {
      "type": ["null", "array"],
      "items": {
        "type": "string"
      }
    },
    "createdBy": {
      "type": ["null", "string"]
    },
    "createdAt": {
      "type": ["null", "string"]
    },
    "apiKey": {
      "type": ["null", "string"]
    },
    "billing": {
      "type": ["null", "object"],
      "properties": {
        "quantity": {
          "type": ["null", "integer"]
        },
        "ok": {
          "type": ["null", "boolean"]
        },
        "plan": {
          "type": ["null", "string"]
        }
      }
    }
  }

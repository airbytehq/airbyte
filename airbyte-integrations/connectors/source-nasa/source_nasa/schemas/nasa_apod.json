{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "resource": {
      "type": ["null", "object"],
      "properties": {
        "image_set": {
          "type": ["null", "string"]
        },
        "planet": {
          "type": ["null", "string"]
        }
      }
    },
    "concept_tags": {
      "type": ["null", "boolean"]
    },
    "title": {
      "type": ["null", "string"]
    },
    "date": {
      "type": ["null", "string"],
      "format": "%Y-%m-%d"
    },
    "url": {
      "type": ["null", "string"],
      "format": "uri"
    },
    "hdurl": {
      "type": ["null", "string"],
      "format": "uri"
    },
    "media_type": {
      "type": ["null", "string"],
      "enum": ["image", "video"]
    },
    "explanation": {
      "type": ["null", "string"]
    },
    "concepts": {
      "type": ["null", "object", "string"],
      "patternProperties": {
        "^[0-9]+$": {
          "type": ["null", "string"]
        }
      }
    },
    "thumbnail_url": {
      "type": ["null", "string"],
      "format": "uri"
    },
    "copyright": {
      "type": ["null", "string"]
    },
    "service_version": {
      "type": ["null", "string"],
      "pattern": "^v[0-9]$"
    }
  }
}

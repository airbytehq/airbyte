{
  "documentationUrl": "https://docs.airbyte.io/integrations",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Dixa Spec",
    "type": "object",
    "required": ["start_date"],
    "additionalProperties": false,
    "properties": {
      "api_token": {
        "type": "string",
        "description": "Dixa API token",
        "airbyte_secret": true
      },
      "start_date": {
        "type": "string",
        "description": "Start date from which to query data.",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
        "examples": ["2021-06-25"]
      }
    }
  }
}

{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Mercado Libre Spec",
    "type": "object",
    "required": ["credstash_key", "client_id", "merchant_code", "client_secret", "year_month", "google_project_id", "google_secret_aws_credstash_credentials", "aws_access_key_id", "aws_secret_access_key"],
    "additionalProperties": false,
    "properties": {
      "credstash_key": {
        "title": "credstash_key",
        "type": "string",
        "description": "credstash_key",
        "airbyte_secret": true
      },
      "client_id": {
        "title": "client_id",
        "type": "string",
        "description": "client_id"
      },
      "merchant_code": {
        "title": "merchant_code",
        "type": "string",
        "description": "Merchant_code. Example: BRABC"
      },
      "client_secret": {
        "title": "client_secret",
        "type": "string",
        "description": "client_secret",
        "airbyte_secret": true
      },
      "year_month": {
        "title": "year_month",
        "type": "string",
        "description": "year_month date in format YYYYMM"
      },
      "google_project_id": {
        "title": "google_project_id",
        "type": "string",
        "description": "google_project_id date in format YYYYMM"
      },
      "google_secret_aws_credstash_credentials": {
        "title": "google_secret_aws_credstash_credentials",
        "type": "string",
        "description": "google_secret_aws_credstash_credentials date in format YYYYMM",
        "airbyte_secret": true
      },
      "aws_access_key_id": {
        "title": "aws_access_key_id",
        "type": "string",
        "description": "aws_access_key_id date in format YYYYMM",
        "airbyte_secret": true
      },
      "aws_secret_access_key": {
        "title": "aws_secret_access_key",
        "type": "string",
        "description": "aws_secret_access_key date in format YYYYMM",
        "airbyte_secret": true
      }
    }
  }
}

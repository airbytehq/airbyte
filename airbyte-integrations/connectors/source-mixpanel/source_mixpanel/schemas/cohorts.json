{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "name": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "created": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "count": {
      "type": ["null", "integer"]
    },
    "is_visible": {
      "type": ["null", "integer"]
    },
    "project_id": {
      "type": ["null", "integer"]
    }
  }
}

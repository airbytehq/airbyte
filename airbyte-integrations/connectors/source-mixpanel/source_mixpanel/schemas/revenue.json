{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "date": {
      "type": ["null", "string"],
      "format": "date"
    },
    "datetime": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "count": {
      "type": ["null", "integer"]
    },
    "paid_count": {
      "type": ["null", "integer"]
    },
    "amount": {
      "type": ["null", "number"],
      "multipleOf": 1e-20
    }
  }
}

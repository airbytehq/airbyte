{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Shipstation Spec",
    "type": "object",
    "required": ["api_key", "api_secret"],
    "additionalProperties": false,
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The Shipstation username for Basic Authentication"
      },
      "api_secret": {
        "type": "string",
        "description": "The Shipstation password for Basic Authentication",
        "airbyte_secret": true
      }
    }
  }
}

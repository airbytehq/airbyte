{
  "documentationUrl": "https://docs.airbyte.io/integrations/destinations/intercom",
  "supported_destination_sync_modes": [
    "overwrite",
    "append", 
    "append_dedup"
  ],
  "supportsIncremental": true,
  "supportsDBT": false,
  "supportsNormalization": false,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Intercom",
    "type": "object",
    "required": ["access_token", "model"],
    "additionalProperties": true,
    "properties": {
      "access_token": {
        "title": "App access token",
        "type": "string",
        "description": "Intercom access token from the app developer hub",
        "airbyte_secret": true,
        "order": 1
      },
      "model": {
        "title": "Intercom object model",
        "type": "string",
        "description": "Object model to sync, e.g. contact, company",
        "enum": ["contact", "company"],
        "airbyte_secret": false,
        "order": 2
      }
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "sid": {
      "type": ["null", "string"]
    },
    "account_sid": {
      "type": ["null", "string"]
    },
    "date_created": {
      "format": "date-time",
      "type": ["null", "string"]
    },
    "date_updated": {
      "format": "date-time",
      "type": ["null", "string"]
    },
    "friendly_name": {
      "type": ["null", "string"]
    },
    "status": {
      "type": ["string", "null"],
      "enum": ["published", "draft"]
    },
    "version": {
      "type": ["null", "integer"]
    },
    "url": {
      "type": ["null", "string"]
    },
    "links": {
      "type": ["null", "object"],
      "additionalProperties": true,
      "properties": {
        "steps": { "type": ["null", "string"] }
      }
    }
  }
}

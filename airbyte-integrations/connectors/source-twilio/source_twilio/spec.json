{
  "documentationUrl": "https://hub.docker.com/r/airbyte/source-twilio",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Twilio Spec",
    "type": "object",
    "required": ["account_sid", "auth_token", "start_date"],
    "additionalProperties": false,
    "properties": {
      "account_sid": {
        "title": "Account ID",
        "description": "Twilio account SID",
        "airbyte_secret": true,
        "type": "string"
      },
      "auth_token": {
        "title": "Auth Token",
        "description": "Twilio Auth Token.",
        "airbyte_secret": true,
        "type": "string"
      },
      "start_date": {
        "title": "Replication Start Date",
        "description": "UTC date and time in the format 2020-10-01T00:00:00Z. Any data before this date will not be replicated.",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$",
        "examples": ["2020-10-01T00:00:00Z"],
        "type": "string"
      }
    }
  },
  "supportsIncremental": true,
  "supported_destination_sync_modes": ["append"]
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "integer"
    },
    "_self": {
      "type": "string"
    },
    "name": {
      "type": ["string", "null"]
    },
    "order": {
      "type": "integer"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "modified": {
      "type": "string",
      "format": "date-time"
    }
  }
}

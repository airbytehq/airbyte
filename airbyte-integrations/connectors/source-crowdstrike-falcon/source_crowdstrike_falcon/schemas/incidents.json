      "type": "string"
    },
    "end": {
      "type": "string"
    },
    "status": {
      "type": "integer"
    },
    "tactics": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "techniques": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "modified_timestamp": {
      "type": "string"
    },
    "fine_score": {
      "type": "integer"
    }
  },
  "required": [
    "incident_id",
    "host_ids",
    "hosts",
    "created",
    "start",
    "end",
    "status",
    "tactics",
    "techniques",
    "modified_timestamp",
    "fine_score"

{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": true,
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "type": {
        "type": "string"
      },
      "status": {
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "iteration": {
        "type": "string"
      },
      "projects": {
        "type": "array",
        "items": {"type": "string"}
      },
      "createdAt": {
        "type": "string"
      },
      "dueOn": {
        "type": "string"
      },
      "labels": {
        "type": "array",
        "items": {"type": "string"}
      },
      "time": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer"
          },
          "users": {
            "type": ["null", "object"]
          },
          "timerTime": {
            "type": "integer"
          }
        }
      },
      "completed": {
        "type": "boolean"
      },
      "completedAt": {
        "type": "string"
      },
      "assignees": {
        "type": "array",
        "items": {"type": "string"}
      }
    }
  }
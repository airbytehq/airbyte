  "properties": {
      "id": {
          "type": "string"
      },
      "isSubscriber": {
          "type": "boolean"
      },
      "email": {
          "type": ["string", "null"]
      },
      "phone": {
          "type": ["string", "null"]
      },
      "companyName": {
          "type": ["string", "null"]
      },
      "address": {
          "type": "object",
          "properties": {
              "street": {
                "type": ["string", "null"]
              },
              "city": {
                "type": ["string", "null"]
              },
              "postcode": {
                "type": ["string", "null"]
              },
              "country": {
                "type": ["string", "null"]
              },
              "countryCode": {
                "type": ["string", "null"]
              }
          }
      },
      "description": {
          "type": "object",
          "properties": {

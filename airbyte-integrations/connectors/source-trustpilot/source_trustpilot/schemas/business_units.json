                "type": ["string", "null"]
              },
              "text": {
                "type": ["string", "null"]
              }
          }
      },
      "socialmedia": {
          "type": "object",
          "properties": {
              "facebook": {
                "type": ["string", "null"]
              },
              "linkedin": {
                "type": ["string", "null"]
              },
              "twitter": {
                "type": ["string", "null"]
              },
              "googleplus": {
                "type": ["string", "null"]
              },
              "youtube": {
                "type": ["string", "null"]
              },
              "instagram": {
                "type": ["string", "null"]
              }
          }
      },
      "facebookPageId": {
        "type": ["string", "null"]
      },
      "facebookPageUrl": {
        "type": ["string", "null"]
      },
      "isFacebookActivated": {
        "type": "boolean"
      },
      "isCommentsEnabled": {
        "type": "boolean"
      },
      "isIncentivisingUsers": {
        "type": "boolean"
      },
      "isClaimed": {
        "type": "boolean"
      }

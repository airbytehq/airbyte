      "type": "integer"
    },
    "id_lang": {
      "type": "string"
    },
    "id_shop": {
      "type": "string"
    },
    "id_customer": {
      "type": "string"
    },
    "id_order": {
      "type": "string"
    },
    "id_product": {
      "type": "string"
    },
    "id_contact": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "token": {

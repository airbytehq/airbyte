{
  "documentationUrl": "https://developers.survicate.com/data-export/",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Survicate Spec",
    "type": "object",
    "required": ["api_key", "survey_id"],
    "additionalProperties": true,
    "properties": {
      "api_key": {
        "type": "string",
        "description": "Survicate API Key.",
        "airbyte_secret": true
      },
      "survey_id": {
        "type": "string",
        "description": "Survey ID",
        "examples": ["a123b456cc789"]
      },
      "start_datetime": {
        "type": "string",
        "description": "Start getting data from that datetime.",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\+[0-9]{2}:[0-9]{2}$",
        "examples": ["2021-01-30T23:45:00+00:00"]
      }
    }
  }
}
{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "id": {
      "type": "integer",
      "default": 0
    },
    "election": {
      "type": ["null", "string"],
      "default": null
    },
    "exclude_from_meetings": {
      "type": "boolean",
      "default": false
    },
    "focus": {
      "type": ["null", "string"],
      "default": ""
    },
    "links": {
      "type": "object",
      "default": {},
      "properties": {
        "person": {
          "type": ["null", "integer"],
          "default": null
        },
        "role": {
          "type": ["null", "integer"],
          "default": null
        }
      }
    }
  }
}

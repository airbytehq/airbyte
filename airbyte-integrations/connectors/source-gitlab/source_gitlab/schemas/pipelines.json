{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "status": {
      "type": ["null", "string"]
    },
    "ref": {
      "type": ["null", "string"]
    },
    "sha": {
      "type": ["null", "string"]
    },
    "web_url": {
      "type": ["null", "string"]
    },
    "created_at": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "updated_at": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    }
  }
}

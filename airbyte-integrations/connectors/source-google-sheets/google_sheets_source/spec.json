{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/google-sheets",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Stripe Source Spec",
    "type": "object",
    "required": ["spreadsheet_id", "credentials_json"],
    "additionalProperties": false,
    "properties": {
      "spreadsheet_id": {
        "type": "string",
        "description": "The ID of the spreadsheet to be replicated."
      },
      "credentials_json": {
        "type": "string",
        "description": "The contents of the JSON service account key. See the <a href=\"https://docs.airbyte.io/integrations/sources/googlesheets\">docs</a> for more information on how to generate this key.",
        "airbyte_secret": true
      }
    }
  }
}

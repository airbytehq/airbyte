{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/braintree",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Braintree Source Spec",
    "type": "object",
    "required": [
      "merchant_id",
      "public_key",
      "start_date",
      "private_key",
      "environment"
    ],
    "additionalProperties": false,
    "properties": {
      "merchant_id": {
        "type": "string",
        "description": "<a href=\"https://articles.braintreepayments.com/control-panel/important-gateway-credentials#merchant-id\">Merchant ID</a> is the unique identifier for entire gateway account."
      },
      "public_key": {
        "type": "string",
        "description": "This is your user-specific <a href=\"https://articles.braintreepayments.com/control-panel/important-gateway-credentials#public-key\">public identifier</a> for Braintree"
      },
      "start_date": {
        "type": "string",
        "description": "The date from which you'd like to replicate data for Braintree API, in the format YYYY-MM-DDT00:00:00Z. All data generated after this date will be replicated.",
        "examples": ["2020-11-16T00:00:00Z"],
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
      },
      "private_key": {
        "type": "string",
        "description": "This is your user-specific <a href=\"https://articles.braintreepayments.com/control-panel/important-gateway-credentials#private-key\">private identifier</a>.",
        "airbyte_secret": true
      },
      "environment": {
        "type": "string",
        "description": "Environment specifies where the data will come from - Sandbox or Production environment.",
        "enum": ["Production", "Sandbox"]
      }
    }
  }
}

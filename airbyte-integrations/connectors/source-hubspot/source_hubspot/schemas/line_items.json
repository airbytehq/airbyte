{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": ["object"],
  "required": ["id"],
  "properties": {
    "id": {
      "type": ["string"]
    },
    "createdAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "updatedAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "archived": {
      "type": ["null", "boolean"]
    }
  }
}

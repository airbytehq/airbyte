{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "maxLength": 13,
      "readOnly": true
    },
    "object": {
      "type": ["null", "string"]
    },
    "account_id": {
      "type": "string",
      "maxLength": 13
    },
    "user": {
      "$ref": "users.json"
    },
    "message": {
      "type": ["null", "string"],
      "maxLength": 2048
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    }
  }
}

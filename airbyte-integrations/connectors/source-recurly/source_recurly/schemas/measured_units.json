{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "string"],
      "maxLength": 13
    },
    "name": {
      "type": ["null", "string"],
      "maxLength": 256
    },
    "display_name": {
      "type": ["null", "string"],
      "maxLength": 255
    },
    "state": {
      "type": ["null", "string"],
      "maxLength": 255
    },
    "description": {
      "type": ["null", "string"],
      "maxLength": 1024
    },
    "created_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "updated_at": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "deleted_at": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}
